<h2>Edit itinerary</h2>

  <div class="row col-10 justify-content-center newpage">
      <%= simple_form_for @itinerary do |f| %>
      <%= f.input :title %>
      <%= f.input :city_id %>
      <%= f.input :start_date %>
      <%= f.input :end_date %>
      <%= f.input :visibility %>
      <%= f.submit class: "btn btn-warning" %>
    <% end %>
  </div>
